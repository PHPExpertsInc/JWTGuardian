{
    "name": "phpexperts/jwt-guardian",
    "description": "A full JWT authorization framework for Laravel.",
    "keywords": [
        "api",
        "authentication",
        "laravel auth",
        "jwt",
        "jwt-auth"
    ],
    "homepage": "https://github.com/PHPExpertsInc/JWTGuardian",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Experts, Inc.",
            "homepage": "https://www.phpexperts.pro/"
        },
        {
            "name": "Theodore R. Smith",
            "homepage": "https://www.linkedin.com/in/tedrsmith"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "illuminate/auth": "^5.6|^6|^7",
        "illuminate/support": "^5.6|^6|^7",
        "illuminate/http": "^5.6|^6|^7",
        "tymon/jwt-auth": "1.*",
        "phpexperts/jwt-helper": "^0.6.1",
        "phpexperts/conciseuuid": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*|7.*|8.*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/var-dumper": "*",
        "povils/phpmnd": "*",
        "squizlabs/php_codesniffer": "*",
        "laravel/framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "PHPExperts\\JWTGuardian\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "PHPExperts\\JWTGuardian\\Tests\\" : "tests/" }
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "platform": {
            "php": "7.1.3"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PHPExperts\\JWTGuardian\\JWTGuardianServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
